{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "moduleResolution": "node",
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ]
    },
    "typeAcquisition": {
        "enable": true
    },
    "include": [
        "./**/*.vue",
        "./**/*.ts",
        "./**/*.tsx"
    ]
}
