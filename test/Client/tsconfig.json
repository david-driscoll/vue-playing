{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "target": "es3",
        "module": "es2015",
        "moduleResolution": "node",
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ]
    },
    "typeAcquisition": {
        "enable": true
    },
    "include": [
        "./**/*.vue",
        "./**/*.ts",
        "./../../src/Client/**/*.ts"
    ]
}