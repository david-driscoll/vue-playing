{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "target": "es3",
        "module": "esnext",
        "moduleResolution": "node",
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ]
    },
    "typeAcquisition": {
        "enable": true
    },
    "include": [
        "./**/*.ts",
        "./../client/**/*.ts",
        "./../client/**/*.tsx",
        "./../client/**/*.vue"
    ]
}
